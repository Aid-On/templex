{
  "name": "@aid-on/templex",
  "version": "0.2.1",
  "description": "Templex - Template Extractor for extracting abstract templates and document structures from articles and text",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Aid-On/templex.git"
  },
  "bugs": {
    "url": "https://github.com/Aid-On/templex/issues"
  },
  "homepage": "https://github.com/Aid-On/templex",
  "author": "Aid-On",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run tests/unit",
    "test:watch": "vitest tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:all": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "llm",
    "template",
    "extraction",
    "document-structure",
    "abstraction",
    "text-processing",
    "article-analysis",
    "ai"
  ],
  "license": "MIT",
  "dependencies": {
    "@aid-on/fractop": "^0.6.2",
    "@aid-on/iteratop": "^0.3.1",
    "@aid-on/unillm": "^0.5.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
